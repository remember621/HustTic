@charset "utf-8";
/* CSS Document */

#illustrat{
	position:relative;
	float:left;
	background-image:url(../images/department/department_ilu.png);
	background-repeat:no-repeat;
	width:186px;
	height:90px;
	top:40px;
	left:65px;
	z-index:2;
}
#content1{
	position:relative;
	width:auto;
	height:auto;
}
#content2{
	position:relative;
	width:880px;
	height:720px;
}
#content3{
	position:relative;
	width:auto;
	height:auto;
}
#bj{
	position:relative;
	left:60px;
	z-index:0;
}
#changebj{
	position:absolute;
	z-index:1;
}
#patch1{
	position:relative;
	width:779px;
	height:auto;
	background-color:#fefefe;
	margin-bottom:100px;
	margin-left:70px;
	margin-top:10px;
}
#patch1 .b_left{
	float:left;
}
#patch1 .b_right{
	float:right;
}
.picshow{
	float:left;
	background:#fff;
	width:140px;
	margin:30px 0px 10px 12px;
}
.picshow img{
	border:#f6f1d8 2px solid;
	width:136px;
	height:169px;
}
.shadow{
	background: transparent url(../images/department/shadow.jpg) no-repeat;
	width:140px;
	height:7px;
}
.picshow h2{
	font:"黑体" 12px;
	font-weight:bold;
	padding:5px;
}
.picshow p{
	font:12px "宋体";
	padding:5px;
}